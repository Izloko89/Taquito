    <Button
        android:layout_width="128dp"
        android:layout_height="128dp"
        android:scaleType="fitXY"
        android:background="@drawable/reg_vis"
        android:id="@+id/reg_vis"
        android:layout_alignParentBottom="true"
        android:onClick="CheckIn"/>

    <ImageView
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:src="@drawable/buscar"
        android:scaleType="fitXY"
        android:id="@+id/buscar"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/reg_vis"
        android:layout_marginBottom="19dp"
        android:layout_marginLeft="25dp"
        android:onClick="BusCar"/>

    <ImageView
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:src="@drawable/qhayc"
        android:scaleType="fitXY"
        android:id="@+id/qhayc"
        android:layout_alignTop="@+id/buscar"
        android:layout_toRightOf="@+id/buscar"
        android:layout_marginLeft="25dp" />

    <ImageView
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:src="@drawable/amigos"
        android:scaleType="fitXY"
        android:id="@+id/amigos"
        android:layout_alignTop="@+id/qhayc"
        android:layout_toRightOf="@+id/qhayc"
        android:layout_marginLeft="25dp"
        android:onClick="AmiGos"/>

    <ImageView
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:src="@drawable/perfil"
        android:scaleType="fitXY"
        android:id="@+id/perfil"
        android:layout_alignTop="@+id/amigos"
        android:layout_toRightOf="@+id/amigos"
        android:layout_marginLeft="25dp"
        android:onClick="PerFil"/>


    <!--<LinearLayout
